includes:
    - ./vendor/nunomaduro/larastan/extension.neon

parameters:

    paths:
        - app
        - routes

    # The level 9 is the highest level
    level: 6

    ignoreErrors:
        - '#PHPDoc tag @var#'

    excludePaths:
        - ./routes/console.php
        - ./app/Http/Controllers/Auth/VerifyEmailController.php
        - ./*/*/FileToBeExcluded.php

    checkMissingIterableValueType: true
